{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|webp|svg|mp4|ico))",
      "headers": { "cache-control": "public,max-age=31536000,immutable" },
      "dest": "/$1"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
